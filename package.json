{
  "name": "react-trail",
  "version": "1.2.0",
  "description": "Declarative routing for React. This package avoid the Routes value repetition and create Links and URLs, with location param parsing.",
  "main": "./dist/Trail.js",
  "repository": "https://github.com/uciadonis/react-trail.git",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "test": "jest",
    "transpile": "babel src -d dist --copy-files",
    "prepublishOnly": "npm run transpile",
    "build": "webpack --mode production && cp examples/public/404.html examples/dist/404.html",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build && npm run deploy"
  },
  "keywords": [
    "trail",
    "react",
    "router",
    "route"
  ],
  "author": "Adonis Montoya",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-testing-library": "^8.0.1",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^2.0.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "yup": "^0.32.9"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "yup": "^0.32.9"
  },
  "dependencies": {
    "querystringify": "^2.2.0",
    "warning": "^4.0.3"
  },
  "jest": {
    "roots": [
      "tests"
    ],
    "testRegex": "\\.js"
  }
}
